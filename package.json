{
  "name": "url-shortner-cli",
  "version": "1.0.0",
  "description": "A TypeScript CLI to parse cron strings and expand field values. No cron parser libraries used.",
  "type": "module",
  "bin": {
    "url-short": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "cron",
    "parser",
    "typescript",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
